package me.mcf5.main;

import org.bukkit.Material;
import org.bukkit.inventory.ItemStack;

public class Debug {
	public static void main(String[] args){
		System.out.println("Startup...");
		ItemStack[] one = new ItemStack[]{item(Material.STONE, 5), item(Material.STONE, 5), item(Material.WOOD, 1)};
		ItemStack[] two	= new ItemStack[]{item(Material.STONE, 1), item(Material.WOOD, 1), item(Material.STONE, 1)};
		
		ArrayI.log(ArrayI.combineEven(one, two));
	}
	
	
	
	
	
	
	
	public static ItemStack item(Material mat, int amount){
		return new ItemStack(mat, amount);
	}
}
